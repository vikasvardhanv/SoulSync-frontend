{
  "name": "soulsync-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "SoulSync - AI-powered dating platform frontend",
  "homepage": "https://soulsync.solutions",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/soulsync-frontend.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host",
    "build:production": "NODE_ENV=production npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "serve": "vite preview",
    "analyze": "npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:vercel": "vercel --prod",
    "start": "npm run serve"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@paypal/react-paypal-js": "^8.8.3",
    "axios": "^1.7.2",
    "framer-motion": "^11.0.8",
    "immer": "^10.1.1",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.4",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.30.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.9.0",
    "@rollup/rollup-darwin-x64": "^4.9.0",
    "@rollup/rollup-linux-x64-gnu": "^4.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
